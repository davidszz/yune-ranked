{
  "name": "yune-ranked",
  "version": "1.0.0",
  "description": "A discord bot client for ranked games",
  "main": "dist/index.js",
  "repository": "https://github.com/sasuke-7/yune-ranked",
  "author": "sasuke-7 <davidkunschventurini@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "set NODE_ENV=production && node .",
    "dev": "set NODE_ENV=development && ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/index.ts --no-deploy",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@types/moment": "^2.13.0",
    "canvas": "^2.9.0",
    "colors": "^1.4.0",
    "discord.js": "^14.0.0-dev.1645402068.78aa36f",
    "dotenv": "^14.3.0",
    "humanize-duration": "^3.27.1",
    "i18next": "^21.6.10",
    "i18next-fs-backend": "^1.1.4",
    "moment": "^2.29.1",
    "mongoose": "^6.1.8",
    "nanoid": "^3.2.0",
    "readdirp": "^3.6.0",
    "transform-props": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/dotenv": "^8.2.0",
    "@types/humanize-duration": "^3.27.1",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/mongoose": "^5.11.97",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
