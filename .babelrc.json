{
	"presets": [
		"@babel/preset-typescript",
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current"
				}
			}
		]
	],
	"plugins": [
		[
			"module-resolver",
			{
				"alias": {
					"@client": "./src/Yune",
					"@database": "./src/database",
					"@utils": "./src/utils",
					"@services": "./src/services",
					"@structures": "./src/structures",
					"@loaders": "./src/loaders"
				}
			}
		]
	]
}